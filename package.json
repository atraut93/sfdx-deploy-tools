{
  "name": "@andrew.trautmann/sfdx-deploy-tools",
  "version": "0.2.1-beta",
  "author": "Andrew Trautmann",
  "bugs": "https://github.com/atraut93/sfdx-deploy-tools/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "^1",
    "@salesforce/command": "^3",
    "@salesforce/core": "^2",
    "mkdirp": "^1.0.3",
    "simple-git": "^2.4.0",
    "strip-color": "^0.1.0",
    "tslib": "^1",
    "xmlbuilder2": "^2.1.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^3",
    "@oclif/test": "^1",
    "@salesforce/dev-config": "^1.5.0",
    "@types/chai": "^4",
    "@types/mocha": "^7",
    "@types/node": "^10",
    "chai": "^4",
    "globby": "^8",
    "mocha": "^7",
    "nyc": "^15",
    "sinon": "^9.0.2",
    "ts-node": "^8",
    "tslint": "^6",
    "typescript": "^3",
    "xpath": "^0.0.27"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/atraut93/sfdx-deploy-tools",
  "keywords": [
    "sfdx-deploy-tools"
  ],
  "license": "MIT",
  "mocha": {
    "recursive": true,
    "reporter": [
      "spec",
      "xunit"
    ],
    "reporterOption": [
      "output=test-results/xunit/results.xml"
    ],
    "require": "ts-node/register",
    "timeout": 15000
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ],
    "report-dir": "./coverage"
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "topics": {
      "deploytools": {
        "description": "Commands to help with deploy process.",
        "subtopics": {
          "deploy": {
            "description": "Commands to help with deploying metadata"
          },
          "retrieve": {
            "description": "Commands to help with retrieving metadata"
          },
          "test": {
            "description": "Commands to help with tests run for a given deploy."
          }
        }
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "atraut93/sfdx-deploy-tools",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
